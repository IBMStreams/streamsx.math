<?xml version="1.0" encoding="UTF-8"?>
<functionModel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.ibm.com/xmlns/prod/streams/spl/function" xmlns:cmn="http://www.ibm.com/xmlns/prod/streams/spl/common" xsi:schemaLocation="http://www.ibm.com/xmlns/prod/streams/spl/function functionModel.xsd">
  <functionSet>
    <headerFileName>supportfunctions.h</headerFileName>
    <!-- <cppNamespaceName></cppNamespaceName> -->
    <functions>
      <function>
        <description>Convert list of 8 uint8 bytes into a float64 wrt IEEE754</description>
        <prototype>public float64 uint8ListToFloat64(list&lt;uint8> x)</prototype>
      </function>
      <function>
        <description>Convert list of 4 uint8 bytes into a float32 wrt IEEE754</description>
        <prototype>public float32 uint8ListToFloat32(list&lt;uint8> x)</prototype>
      </function>
      <function>
        <description>Convert list of 8 uint8 bytes into (signed) int64</description>
        <prototype>public int64 uint8ListToInt64(list&lt;uint8> x)</prototype>
      </function>
      <function>
        <description>Convert list of 8 uint8 bytes into (unsigned) uint64</description>
        <prototype>public uint64 uint8ListToUint64(list&lt;uint8> x)</prototype>
      </function>
      <function>
        <description>Convert list of 4 uint8 bytes into (signed) int32</description>
        <prototype>public int32 uint8ListToInt32(list&lt;uint8> x)</prototype>
      </function>
      <function>
        <description>Convert list of 4 uint8 bytes into (unsigned) uint32</description>
        <prototype>public uint32 uint8ListToUint32(list&lt;uint8> x)</prototype>
      </function>
      <function>
        <description>Convert list of 2 uint8 bytes into (signed) int16</description>
        <prototype>public int16 uint8ListToInt16(list&lt;uint8> x)</prototype>
      </function>
      <function>
        <description>Convert list of 2 uint8 bytes into (unsigned) uint16</description>
        <prototype>public uint16 uint8ListToUint16(list&lt;uint8> x)</prototype>
      </function>
    </functions>
    <dependencies>
      <library>
        <cmn:description>The function library</cmn:description>
        <cmn:managedLibrary>
          <cmn:lib>supportfunctions</cmn:lib>
          <cmn:libPath>../../impl/lib</cmn:libPath>
          <cmn:includePath>../../impl/include</cmn:includePath>
        </cmn:managedLibrary>
      </library>
    </dependencies>
  </functionSet>
</functionModel>
